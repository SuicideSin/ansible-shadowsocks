{
    "server": "{{ ansible_default_ipv4.address }}"
  , "server_port": {{ shadowsocks.config.server_port }}
  , "local_port": {{ shadowsocks.config.local_port }}
  {% if shadowsocks.dynamic_password -%}
  , "password": "{{ shadowsocks_password.stdout }}"
  {% else -%}
  , "password": "{{ shadowsocks.static_password }}"
  {% endif -%}
  , "timeout": {{ shadowsocks.config.timeout }}
  , "method": "{{ shadowsocks.config.encryption_method }}"
  {% if ansible_kernel | version_compare('3.7', '>=') -%}
  , "fast_open": true
  {% else -%}
  , "fast_open": false
  {% endif %}
}
